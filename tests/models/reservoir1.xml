<?xml version="1.0"?>
<pywr>
    <metadata>
        <title>Reservoir 1</title>
        <description>
            Model with a reservoir
        </description>
    </metadata>
    <nodes>
        <reservoir name="supply1" x="1" y="1">
            <parameter type="constant" key="max_volume">35</parameter>
            <parameter type="constant" key="volume">35</parameter>
        </reservoir>
        <river name="link1" x="2" y="1" />
        <demandcentre name="demand1" x="3" y="1">
            <parameter type="constant" key="max_flow">10</parameter>
            <parameter type="constant" key="cost">-10</parameter>
        </demandcentre>
    </nodes>
    <edges>
        <edge from="supply1" to="link1" />
        <edge from="link1" to="demand1" />
    </edges>
</pywr>
