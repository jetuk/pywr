<?xml version="1.0"?>
<pywr>
    <metadata>
        <title>Bottleneck</title>
        <description>
            A very simple example of supply costs.
        </description>
    </metadata>
    <nodes>
        <input name="supply1" x="1" y="1">
            <parameter type="constant" key="max_flow">20</parameter>
        </input>
        <input name="supply2" x="3" y="1">
            <parameter type="constant" key="max_flow">20</parameter>
        </input>
        <link name="link1" x="2" y="2">
            <parameter type="constant" key="max_flow">15</parameter>
        </link>
        <output name="demand1" x="1" y="3">
            <parameter type="constant" key="max_flow">10</parameter>
            <parameter type="constant" key="cost">-10</parameter>
        </output>
        <output name="demand2" x="3" y="3">
            <parameter type="constant" key="max_flow">10</parameter>
            <parameter type="constant" key="cost">-10</parameter>
        </output>
    </nodes>
    <edges>
        <edge from="supply1" to="link1" />
        <edge from="supply2" to="link1" />
        <edge from="link1" to="demand1" />
        <edge from="link1" to="demand2" />
    </edges>
</pywr>
