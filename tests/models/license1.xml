<?xml version="1.0"?>
<pywr>
    <metadata>
        <title>License</title>
        <description>
            A very simple example of a license.
        </description>
    </metadata>
    <nodes>
        <input name="supply1" x="1" y="1">
            <parameter type="constant" key="max_flow">15</parameter>
            <licensecollection>
                <license type="daily">5.0</license>
                <license type="annual">7.0</license>
            </licensecollection>
        </input>
        <link name="link1" x="2" y="1" />
        <output name="demand1" x="3" y="1">
            <parameter type="constant" key="max_flow">10</parameter>
            <parameter type="constant" key="cost">-1</parameter>
        </output>
    </nodes>
    <edges>
        <edge from="supply1" to="link1" />
        <edge from="link1" to="demand1" />
    </edges>
</pywr>
